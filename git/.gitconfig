[user]
	name = Erin
	email = github@eritbh.me
[alias]
	l = log --oneline --decorate --graph
	la = log --oneline --decorate --graph --all
	wmerge = merge --no-commit -Xignore-space-change
	lc = diff --numstat
	pushup = push --set-upstream origin @
	s = stash save --include-untracked
	sp = stash pop
	sl = stash list
[pull]
	rebase = true
[init]
	defaultBranch = main
